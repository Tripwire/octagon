      "preset": "angular"
    },
    "verifyConditions": "condition-circle"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Tripwire/octagon.git"
  },
  "version": "0.0.0-development",
  "lint-staged": {
    "linters": {
      "src/**/*.js": [
        "prettier-standard",
        "git add"
      ],
      "src/**/*.jsx": [
        "prettier-standard",
        "git add"
      ],
      "scripts/**/*.jsx": [
        "prettier-standard",
        "git add"
      ],
      "*.js'": [
        "prettier-standard",
        "git add"
      ]
    }
